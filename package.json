{
  "name": "reflexbox",
  "version": "3.0.0",
  "description": "Responsive React Flexbox Grid System",
  "main": "dist/index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p",
    "start": "webpack-dev-server",
    "prepublish": "mkdir -p dist && babel src --out-dir dist",
    "test": "ava",
    "card": "repng docs/Card.js --dev"
  },
  "author": "Brent Jackson",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.3",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "browser-env": "^3.2.4",
    "chroma-js": "^1.3.4",
    "funcup": "^1.0.0-0",
    "raw-loader": "^0.5.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-live": "^1.5.3",
    "react-test-renderer": "^15.5.4",
    "react-x-ray": "^1.0.0-1",
    "reline": "^1.0.0-beta.3",
    "repng": "^2.0.0-alpha.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jxnblk/reflexbox.git"
  },
  "keywords": [
    "react",
    "react-component",
    "flexbox",
    "layout",
    "grid",
    "css-in-js"
  ],
  "bugs": {
    "url": "https://github.com/jxnblk/reflexbox/issues"
  },
  "babel": {
    "presets": [
      "env",
      "stage-0",
      "react"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "homepage": "https://github.com/jxnblk/reflexbox#readme",
  "dependencies": {
    "prop-types": "^15.5.10"
  }
}
